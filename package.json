  "version": "1.0.0",
  "private": true,
  "author": "Nils Neumann <mail@der-zyklop.com>",
  "devDependencies": {
    "bower": "^1.3.3",
    "grunt": "~0.4.4",
    "grunt-autoprefixer": "3.0.x",
    "grunt-blink1": "^0.3.1",
    "grunt-concurrent": "^2.1.0",
    "grunt-contrib-coffee": "0.13.x",
    "grunt-contrib-compress": "^0.14.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "0.8.x",
    "grunt-contrib-cssmin": "0.14.x",
    "grunt-contrib-imagemin": "1.0.x",
    "grunt-contrib-sass": "0.9.x",
    "grunt-contrib-uglify": "0.11.x",
    "grunt-contrib-watch": "0.6.x",
    "grunt-eslint": "~17.3.1",
    "grunt-image-embed": "~0.3.1",
    "grunt-newer": "~1.1.1",
    "grunt-notify": "^0.4.3",
    "grunt-php": "1.5.x",
    "grunt-shell": "~1.1.2",
    "matchdep": "1.0.x"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "grunt"
  },
  "dependencies": {
    "time-grunt": "~1.3.0"
  },
  "bugs": {
    "url": "https://github.com/DerZyklop/der-zyklop.de/issues"
  },
  "paths": {
    "root": "",
    "src": {
      "dir": "assets/src/",
      "coffee": "assets/src/coffee/",
      "js": "assets/src/js/",
      "js_concat": [
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/fancybox/source/jquery.fancybox.js",
        "bower_components/prism/prism.js",
        "bower_components/jquery-autosize/jquery.autosize.min.js",
        "assets/pre_js/*.js"
      ],
      "sass": "assets/src/sass/",
      "css": "assets/src/styles/",
      "css_concat": [
        "assets/pre_css/*.css"
      ]
    },
    "build": {
      "js": "assets/js/",
      "css": "assets/styles/"
    }
  }
}
